#include <stdio.h>
#include <string.h>

int main(int argc, char *argv[]){
    char words[101];
    scanf("%s", words);
    int n = strlen(words);
    for(int i = 0; i < n / 2; i++){
        if(words[i] != words[n - 1 - i]){
           printf("NO");
            break;
        }else
    {
        printf("YES");
        break;
    }
    }
}
